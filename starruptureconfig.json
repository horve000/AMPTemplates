  "Description": "StarRupture dedicated server using SteamCMD",
  "Author": "Custom",
  "Version": "1.0",

  "DeploymentTemplate": true,
  "IsGameServer": true,

  "RequiresSteamCMD": true,
  "SteamAppID": 3247030,
